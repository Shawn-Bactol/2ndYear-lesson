public class Student1{
     String name;
     String course;
     int age;

     public Student1(){
       System.out.println("Constructor 1 called: default assigned.");
       name = "Unknown";
       age = 0;
       course = "undeclared";
     }
    public Student1(String name){
        System.out.println("Constructor 2 called: Name is assigned.");
        this.name = name;
        age = 18;
        course = "undeclared";
    }
    public Student1(String name, int age){
        System.out.println("Constructor 3 called: Name and age assigned. ");
        this.name = name;
        this.age=age;
        course="undeclared";
    }
    public Student1(String name, int age, String course){
        System.out.println("Constructor 4 called: Name, age, and course assigned.");
        this.name=name;
        this.age=age;
        this.course=course;
    }
    public void display(){
     System.out.println("Student info -> Name: "+name+", Age: "+age+", Course: "+course);
    }

    public static void main(String[] args) {
        
        Student1 s1 = new Student1();
        // Student s2 = new Student("alice");
        // Student s3 = new Student("Bob", 22);
        // Student s4 = new Student("Shawn", 20, "BSIT");
        s1.display();
        // s2.display();
        // s3.display();
        // s4.display();
    }
}
