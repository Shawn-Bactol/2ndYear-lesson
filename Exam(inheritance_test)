public class Exam {
    String message;
    double price;
    boolean status;
    
    public Exam(){
      message="Good luck";
    }
    public Exam(String period, String level){
    }
    public double getPrice(){
    return price;
    }
    public boolean isFinished(){
    return status;
    }

    public static void main(String[] args) {
        Exam exam = new Exam();
        Midterm midterm = new Midterm();
        System.out.println(exam.message);

        Essay essay = new Essay();
        essay.price=0.0;
        essay.status=true;

        System.out.println("Essay inherits from exam: "+essay.isFinished());
        Quiz quiz = new Quiz();

    }
}
class Midterm extends Exam{
  public Midterm(){
    super();
    System.out.println("Exam has started.");
  }
}
class Essay extends Exam{

}
class Quiz{

}
